{
    "name": "skinoniks/laravel-modules",
    "description": "Laravel Module management",
    "keywords": [
        "modules",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^7.0",
        "laravel/framework": "^9.0",
        "laravel/pint": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "SkinonikS\\Laravel\\Modules\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "SkinonikS\\Laravel\\Modules\\ModulesServiceProvider"
            ],
            "aliases": {
                "Modules": "SkinonikS\\Laravel\\Modules\\Facades\\Modules"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}